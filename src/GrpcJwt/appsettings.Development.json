{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "JwtOptions": {
    "Issuer": "GrpcJwt",
    "Audience": "GrpcJwt",
    "AccessExpiration": 1440,
    "RefreshExpiration": 1440
  }
}
